/*
 * ImageBoard 테이블에 대한 CRUD 만을 전담하는 DAO 정의
 * */

package board.model;

import db.DBManager;

public class ImageBoardDAO {
	DBManager dbManager = new DBManager(); //ImageBoardDAO 인스턴스가 생성될떄 
	
	
	//create(insert)
	public void insert() {
		String sql = "insert into imageboard(author,title,content,filename)";
		
	}
	
	//selectALL()
	public void selectAll() {
		String sql = "select * from imageboard";
	}
	//select(one)
	public void select() {
		String sql = "select * from imageboard where board_id = ?";
	}
	//update
	public void update() {
		String sql = "update imageboard set author = ? , title = ? , content = ? where board_id = ?";
	}
	
	//delete
	public void delete() {
		String sql = "delete from where board_id = ?";
	}
}
